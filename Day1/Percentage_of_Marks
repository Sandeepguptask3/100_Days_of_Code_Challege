package com.company;
import java.util.Scanner;
public class marks_percentage {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Eneter your maths marks =");
        int math = sc.nextInt();
        System.out.println("Eneter your sci marks =");
        int sci = sc.nextInt();
        System.out.println("Eneter your sst marks =");
        int sst = sc.nextInt();
        System.out.println("Eneter your hindi marks =");
        int hindi = sc.nextInt();
        System.out.println("Eneter your english marks =");
        int english = sc.nextInt();
        float percentage=( (math +  sci + sst + hindi + english )/500.0f) * 100 ;
        System.out.println("percenatge = ");
        System.out.println(percentage);



        }
    }

